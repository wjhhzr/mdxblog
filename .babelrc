{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        // "@babel/plugin-transform-runtime",
        //增加新的插件，这个插件就是让antd可以按需引入，包括CSS   
        [
            "import",
            {
                "libraryName": "antd"
            }
        ],
        [
            "babel-plugin-styled-components",
            {
              "ssr": true
            }
          ]
    ]
}